<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>projetIndus@AddressBookMultiAccountsManager.com</em:id>
    <em:name>AddressBook Multi Accounts Manager</em:name>
    <em:description>This project aims at allowing ThunderBird's users who have multiple accounts
to match a contact from the AddressBook to one ThunderBird Account. An alert
is displayed when the user sends an email to a contact which is not matched 
with the selected account. There is also a feature into the preferences panel, 
which allows to automatically fecth a user account to your contacts by browsing
your email.</em:description>
    <em:version>0.1</em:version>
    <em:creator>Baptiste Meynier</em:creator>
    <em:creator>Johan Jans</em:creator>
    <em:creator>Maxime Denoyer</em:creator>
    <em:creator>Mustapha Oucheikh</em:creator>
    <em:homepageURL>http://baptistemeynier.github.com/ThunderBird_Multi_Accounts_Manager/</em:homepageURL>
    <em:optionsURL>chrome://AddressBookMultiAccountsManager/content/preferencesWindow.xul</em:optionsURL>
    <em:type>2</em:type>

    <!-- Mozilla ThunderBird-->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>10.0</em:minVersion>
        <em:maxVersion>12.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
